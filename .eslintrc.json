{
  "extends": "next/core-web-vitals",
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "import/no-named-as-default": 0,
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ],
    "camelcase": "off",
    "no-console": "off",
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "never"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "no-var": [
      "off"
    ],
    "one-var": [
      "off"
    ]
  }
}